  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/karthmoh-nitt/mygithubactions/issues"
  },
  "homepage": "https://github.com/karthmoh-nitt/mygithubactions#readme",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2"
  }
}

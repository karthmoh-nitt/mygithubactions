{
  "name": "mygithubactions",
  "version": "1.0.0",
  "description": "This is to setup and test github actions to run Playwright tests",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karthmoh-nitt/mygithubactions.git"
  },
  "keywords": [],
